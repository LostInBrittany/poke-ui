{
  "entrypoint": "index.html",
  "shell": "bundle.html",
  "fragments": [],
  "sources": [
    "bower_components/font-roboto/fonts/**",
    "index.html",
    "bundle.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "bower_components/webcomponentsjs/**",
    "bower_components/d3/**",
    "bower_components/c3/**"
  ],
  "builds": [
    {
      "name": "bundled",
      "bundle": true,
      "addServiceWorker": true,
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    },
    {
      "name": "unbundled",
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ]
}